{
  "name": "ubitech",
  "version": "1.0.0",
  "description": "A starting project for ubitech vue front end",
  "productName": "PLEIONE",
  "author": "Niki Kontoe <nkontoe@ubitech.eu>",
  "private": true,
  "scripts": {
    "serve": "quasar dev",
    "dev": "QENV=DEV quasar dev",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "build": "set QENV=PROD && quasar build",
    "prod": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.10",
    "@tanstack/vue-query": "^4.27.0",
    "axios": "^1.3.1",
    "keycloak-js": "^19.0.1",
    "moment-timezone": "^0.5.40",
    "pinia": "^2.0.30",
    "pinia-shared-state": "^0.3.0",
    "quasar": "^2.12.7",
    "vue": "^3.4.27",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@quasar/app-vite": "^1.2.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.2.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitest/ui": "^0.29.2",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.17.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
